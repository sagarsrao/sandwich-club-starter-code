<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="textViewStyle">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd" tools:targetApi="jelly_bean_mr1">
            @dimen/constraint_right
        </item>
        <item name="android:layout_marginLeft" tools:targetApi="jelly_bean_mr1">
            @dimen/constraint_left
        </item>
        <item name="android:layout_marginStart" tools:targetApi="jelly_bean_mr1">@dimen/constraint_left</item>

        <item name="android:layout_marginRight">@dimen/constraint_right</item>
        <item name="android:layout_marginTop">@dimen/constraint_top</item>


    </style>


</resources>
